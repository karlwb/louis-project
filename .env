# Genesys Cloud Credentials
# Replace with your specific Genesys Cloud region (e.g., mypurecloud.com, mypurecloud.ie, etc.)
GENESYS_CLOUD_REGION="mypurecloud.com"

# Get these from your Genesys Cloud OAuth Client Credentials Grant configuration
GENESYS_CLOUD_CLIENT_ID="your-client-id-goes-here"
GENESYS_CLOUD_CLIENT_SECRET="your-client-secret-goes-here"

# The name of the queue you want to monitor
TARGET_QUEUE_NAME="your-target-queue-name"


##### MTA 

# The full URL to the backend service that returns the list of tickets
MTA_QUEUE_TICKET_URL="https://your-internal-mta-service.com/api/tickets"

# The Bearer token for authentication
MTA_BEARER_TOKEN="your-bearer-token-goes-here"
